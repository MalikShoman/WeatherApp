        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@drawable/bottomlayout"
            android:layout_marginTop="18dp">


<!--            <ListView-->
<!--                android:id="@+id/lst_days"-->
<!--                android:layout_width="match_parent"-->
<!--                android:layout_height="match_parent"-->
<!--                />-->

            <TextView
                android:id="@+id/next_day"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_gravity="center"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="174dp"
                android:text="Next Day"
                android:textSize="22dp"
                android:textColor="@color/colorOffWhite" />

            <ImageView
                android:id="@+id/next_day_icon"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_below="@+id/next_day"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="21dp"
                android:src="@drawable/cloud" />


            <TextView
                android:layout_width="90dp"
                android:id="@+id/next_max"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="10dp"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="300dp"
                android:textSize="28dp"
                android:text="Max"
                android:textColor="@color/white"
                android:layout_toStartOf="@id/second_day_maxTemp" />

            <TextView
                android:id="@+id/second_day_maxTemp"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="80dp"
                android:layout_marginEnd="50dp"
                android:layout_marginBottom="300dp"
                android:layout_toStartOf="@+id/next_day_icon"
                android:layout_weight="1"
                android:text="12°"
                android:textSize="28dp"
                android:textColor="@color/colorWhite" />


            <TextView
                android:id="@+id/next_min"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="255dp"
                android:text="Min"
                android:textColor="@color/white"
                android:layout_toStartOf="@id/second_day_maxTemp"
                android:textSize="28dp" />

            <TextView
                android:id="@+id/second_day_minTemp"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="50dp"
                android:layout_marginBottom="255dp"
                android:layout_toStartOf="@+id/next_day_icon"
                android:layout_weight="1"
                android:text="12°"
                android:textColor="@color/white"
                android:textSize="28dp" />

        </RelativeLayout>
